{
  "name": "user-stories-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_API_URI_PREFIX=https://api.myuserstories.com react-scripts build",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "test:jest": "react-scripts test --env=jsdom",
    "test-travis": "NODE_ENV=test istanbul cover _mocha -- --compilers js:babel-core/register --recursive ./test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "coveralls": "^2.11.16",
    "istanbul": "^1.1.0-alpha.1",
    "jquery": "^3.1.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.7",
    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "0.8.5",
    "redux-mock-store": "^1.2.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  }
}
